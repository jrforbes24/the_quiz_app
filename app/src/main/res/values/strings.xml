<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">THE QUIZ APP</string>
    <!--Hint for the name field -->
    <string name="hint_name">Name</string>
    <!--Button text for Start-->
    <string name="start_button">START</string>
    <!--This will show the quiz score example: Score 5/10-->
    <string name="quiz_score">Score <xliff:g example="5" id="number_right">%d</xliff:g>/<xliff:g example="10" id="number_possible">%d</xliff:g></string>
    <!--Question text field - Example: Question: What is the highest mountain in the world? -->
    <string name="the_question">Question: <xliff:g example="How many wars did Colonel Potter fight in?" id="question_text">%s</xliff:g></string>
    <!--Answer field 1 this will hold the first possible answer Example:  A. one-->
    <string name="one_answer">A. <xliff:g example="one" id="one_answer">%s</xliff:g></string>
    <!--Answer field 2 this will hold the second possible answer Example:  B. twelve-->
    <string name="two_answer">B. <xliff:g example="twelve" id="two_answer">%s</xliff:g></string>
    <!--Answer field 3 this will hold the third possible answer Example:  C. two-->
    <string name="three_answer">C. <xliff:g example="two" id="three_answer">%s</xliff:g></string>
    <!--Answer field 4 this will hold the third possible answer Example:  D. five-->
    <string name="four_answer">D. <xliff:g example="five" id="four_answer">%s</xliff:g></string>
    <!---->
</resources>
